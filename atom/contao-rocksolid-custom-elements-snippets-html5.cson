            $isEven = true;
            $max = \count($this->list) - 1;
            ?>
            <?php foreach ($this->list as $i => $list): ?>
                <div class="item item_<?= $i ?><?php if ($i === $max): ?> last<?php elseif ($i===0): ?> first<?php endif; ?><?php if ($isEven): ?> even<?php $isEven = false; ?><?php else: ?> odd<?php $isEven = true; ?><?php endif; ?>">
        
                </div>
            <?php endforeach; ?>
        <?php endif; ?>
    """
