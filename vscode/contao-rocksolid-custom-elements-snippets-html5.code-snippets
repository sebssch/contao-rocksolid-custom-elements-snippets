"Tempalte: Rocksolid Custom Element - Liste": {
  "scope": "html5, html",
  "prefix": "cete-list",
  "body": [
    "<?php if (\\$this->list): ?>",
    "    <?php",
    "    \\$isEven = true;",
    "    \\$max = \count(\\$this->list) - 1;",
    "    ?>",
    "    <?php foreach(\\$this->list as \\$i => \\$list): ?>",
    "         <div class=\"item item_<?= \\$i ?><?php if (\\$i === \\$max): ?> last<?php elseif (\\$i===0): ?> first<?php endif; ?><?php if (\\$isEven): ?> even<?php \\$isEven = false; ?><?php else: ?> odd<?php \\$isEven = true; ?><?php endif; ?>\">",
    "",
    "        </div>",
    "    <?php endforeach; ?>",
    "<?php endif; ?>"
  ],
  "description": "Tempalte: Rocksolid Custom Element - Liste"
}
"Tempalte: Rocksolid Custom Element - Searchable": {
  "scope": "html5, html",
  "prefix": "cete-searchable",
  "body": [
    "<?php \\$this->extend('block_searchable'); ?>",
    "",
    "<?php \\$this->block('headline'); ?>",
    "<?php \\$this->endblock(); ?>",
    "",
    "<?php \\$this->block('content'); ?>",
    "<?php \\$this->endblock(); ?>"
  ],
  "description": "Tempalte: Rocksolid Custom Element - Searchable"
