"Tempalte: Rocksolid Custom Element - Checkbox": {
  "scope": "html5, html",
  "prefix": "cete-checkbox",
  "body": [
    "<?php if (\\$this->checkbox):?>",
    "  <?= \\$this->checkbox ?>",
    "<?php endif; ?>"
  ],
  "description": "Tempalte: Rocksolid Custom Element - Checkbox"
}
