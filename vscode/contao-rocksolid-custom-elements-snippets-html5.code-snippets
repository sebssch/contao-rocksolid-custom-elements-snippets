"Tempalte: Rocksolid Custom Element - Bild Contao Basic": {
  "scope": "html5, html",
  "prefix": "cete-image",
  "body": [
    "<?php",
    "\\$image = \\$this->getImageObject(\\$this->singleSRC, \\$this->size);",
    "?>",
    "<?php if (null !== \\$image): ?>",
    "    <?php if (\\$this->fullsize): ?>",
    "        <?php \\$image->fullsize = \\$this->fullsize; ?>",
    "    <?php endif; ?>",
    "    <?php if(\\$this->overwriteMeta): ?>",
    "        <?php \\$image->floating = ' float_' . \\$this->floating; ?>",
    "        <?php \\$image->alt = \\$this->alt; ?>",
    "        <?php \\$image->caption = \\$this->caption; ?>",
    "        <?php if (\\$image->imageUrl): ?>",
    "           <?php \\$image->imageUrl = \\$this->imageUrl; ?>",
    "           <?php \\$image->href = \\$this->imageUrl; ?>",
    "        <?php else: ?>",
    "           <?php",
    "           $parser = \Contao\System::getContainer()->get('contao.insert_tag.parser');",
    "           $image->href = $parser->replace('{{file::' . $this->singleSRC . '}}');",
    "           ?>",
    "        <?php endif; ?>",
    "    <?php endif; ?>",
    "    <?php",
    "    try {",
    "       \\$imageData = json_decode(json_encode(\\$image, JSON_THROW_ON_ERROR), true, 512, JSON_THROW_ON_ERROR);",
    "    } catch (\JsonException \\$exception) {",
    "       \\$imageData = [];",
    "    }",
    "    ?>",
    "    <?php \\$this->insert('image', \\$imageData); ?>",
    "<?php endif; ?>"
  ],
  "description": "Tempalte: Rocksolid Custom Element - Bild Contao Basic"
}
"Tempalte: Rocksolid Custom Element - Bild - Keine Bildgröße": {
  "scope": "html5, html",
  "prefix": "cete-image-nosize",
  "body": [
    "<?php",
    "\\$image = \\$this->getImageObject(\\$this->singleSRC, [ 0, 0, '_imagesize']);",
    "?>",
    "<?php if (null !== \\$image): ?>",
    "    <?php if (\\$this->fullsize): ?>",
    "        <?php \\$image->fullsize = \\$this->fullsize; ?>",
    "    <?php endif; ?>",
    "    <?php if(\\$this->overwriteMeta): ?>",
    "        <?php \\$image->floating = ' float_' . \\$this->floating; ?>",
    "        <?php \\$image->alt = \\$this->alt; ?>",
    "        <?php \\$image->caption = \\$this->caption; ?>",
    "        <?php if (\\$image->imageUrl): ?>",
    "           <?php \\$image->imageUrl = \\$this->imageUrl; ?>",
    "           <?php \\$image->href = \\$this->imageUrl; ?>",
    "        <?php else: ?>",
    "           <?php",
    "           $parser = \Contao\System::getContainer()->get('contao.insert_tag.parser');",
    "           $image->href = $parser->replace('{{file::' . $this->singleSRC . '}}');",
    "           ?>",
    "        <?php endif; ?>",
    "    <?php endif; ?>",
    "    <?php",
    "    try {",
    "       \\$imageData = json_decode(json_encode(\\$image, JSON_THROW_ON_ERROR), true, 512, JSON_THROW_ON_ERROR);",
    "    } catch (\JsonException \\$exception) {",
    "       \\$imageData = [];",
    "    }",
    "    ?>",
    "    <?php \\$this->insert('image', \\$imageData); ?>",
    "<?php endif; ?>"
  ],
  "description": "Tempalte: Rocksolid Custom Element - Bild - Keine Bildgröße"
